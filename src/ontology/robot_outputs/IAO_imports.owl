        <obo:BFO_0000180>MaterialEntity</obo:BFO_0000180>
        <obo:IAO_0000112 xml:lang="en">a flame</obo:IAO_0000112>
        <obo:IAO_0000112 xml:lang="en">a forest fire</obo:IAO_0000112>
