        <rdfs:label>JEOL NMR probe</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OBI_0000530 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OBI_0000530">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OBI_0000597"/>
        <obo:IAO_0000111>Bruker UltraShield Plus NMR magnet</obo:IAO_0000111>
        <obo:IAO_0000114 rdf:resource="http://purl.obolibrary.org/obo/IAO_0000120"/>
        <obo:IAO_0000115 xml:lang="en">An NMR magnet of which Brukers claims it is the latest and most advanced self-shielding NMR magnet technology ever developed. These magnets are the ultimate advancement in high performance, actively-shielded NMR solutions. They offer unprecedented shielding performance whilst ensuring no compromise in system homogeneity, stability or cryogenic specifications.</obo:IAO_0000115>
        <obo:IAO_0000117>PERSON:Daniel Schober</obo:IAO_0000117>
        <obo:IAO_0000119>WEB:&lt;http://www.bruker-biospin.com/nmr_magnets_usplus.html&gt;</obo:IAO_0000119>
