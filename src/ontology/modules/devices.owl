        <obo:IAO_0000117>https://orcid.org/0000-0002-8844-9165</obo:IAO_0000117>
        <obo:IAO_0000118>face covering</obo:IAO_0000118>
        <obo:IAO_0000118>respirator</obo:IAO_0000118>
        <obo:IAO_0000119>Adapted from https://dictionary.cambridge.org/dictionary/english/face-mask</obo:IAO_0000119>
        <obo:IAO_0000233>https://github.com/obi-ontology/obi/issues/1193</obo:IAO_0000233>
        <rdfs:label>face mask</rdfs:label>
    </owl:Class>
    


