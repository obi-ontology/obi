        <obo:IAO_0000119>GROUP:&lt;http://msi-ontology.sourceforge.net&gt;</obo:IAO_0000119>
        <obo:IAO_0000119>http://msi-ontology.sourceforge.net/ontology/CHROM.owl#msi_01106</obo:IAO_0000119>
        <rdfs:label>reversed phase column</rdfs:label>
    </owl:Class>
    


