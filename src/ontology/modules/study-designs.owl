        <obo:IAO_0000117>Hector Guzman-Orozco</obo:IAO_0000117>
        <obo:IAO_0000119>https://en.wikipedia.org/wiki/In_silico</obo:IAO_0000119>
        <obo:IAO_0000233>https://github.com/obi-ontology/obi/issues/1430</obo:IAO_0000233>
        <obo:IAO_0000234>CMI-PB</obo:IAO_0000234>
        <rdfs:label>in silico design</rdfs:label>
    </owl:Class>
    


