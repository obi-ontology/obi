        <obo:IAO_0000233>https://github.com/obi-ontology/obi/issues/1768</obo:IAO_0000233>
        <obo:IAO_0000234>Chris Mungall</obo:IAO_0000234>
        <rdfs:label>ring trial design</rdfs:label>
    </owl:Class>
    


