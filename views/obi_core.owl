        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/iao.owl"/>
        <rdfs:label xml:lang="en">curation status specification</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/IAO_0000100 -->

