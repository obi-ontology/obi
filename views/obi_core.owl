    <!-- http://purl.obolibrary.org/obo/IAO_0000225 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/IAO_0000225">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
