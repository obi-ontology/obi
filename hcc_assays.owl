        <obo:RO_0001900 rdf:resource="http://purl.obolibrary.org/obo/RO_0001901"/>
        <terms:source>http://www.obofoundry.org/ro/#OBO_REL:located_in</terms:source>
        <rdfs:label xml:lang="en">located in</rdfs:label>
    </owl:ObjectProperty>
